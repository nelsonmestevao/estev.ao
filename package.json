{
  "name": "estev.ao",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all develop",
    "develop": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint --fix --ignore-path .gitignore .",
    "lint:css": "stylelint --fix --ignore-path .gitignore './**/*.{css,scss,sass}'",
    "fmt": "npm-run-all format",
    "format": "prettier --ignore-path .gitignore --write './**/*.{css,js,json,md,mdx}'",
    "test": "npm-run-all test:*",
    "test:lint": "npm-run-all test:lint:*",
    "test:lint:js": "eslint --ignore-path .gitignore .",
    "test:lint:css": "stylelint --ignore-path .gitignore './**/*.{css,scss,sass}'",
    "test:format": "prettier --ignore-path .gitignore --check './**/*.{css,js,json,md,mdx}'"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "next": "9.4.4",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "stylelint": "^13.6.1",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-standard": "^20.0.0"
  }
}
